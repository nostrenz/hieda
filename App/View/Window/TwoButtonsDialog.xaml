<Window
	x:Class="Hieda.View.Window.TwoButtonsDialog"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	WindowStartupLocation="CenterOwner"
	Title="TwoButtonsDialog"
	Width="574.288" Height="191.437"
	ResizeMode="NoResize" KeyUp="Window_KeyUp">
	<Grid Background="{StaticResource Window_BackgroundColor}">
		<Button x:Name="button_Yes" Content="Yes" HorizontalAlignment="Left" Margin="10,0,0,10" VerticalAlignment="Bottom" Width="230" Click="Button_Yes_Click" Foreground="#0ebb05"/>
		<Button x:Name="button_No" Content="No" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="230" Click="Button_No_Click" Foreground="#eb0c0c"/>
		<TextBlock x:Name="textBlock_Text" Margin="10,10,10,38" TextWrapping="Wrap" Text="TextBlock" Foreground="{StaticResource Collection_TextColor}" TextAlignment="Center"/>
	</Grid>
</Window>
